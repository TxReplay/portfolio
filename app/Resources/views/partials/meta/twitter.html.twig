<meta name="twitter:card" content="{% block metaTwitterCard %}summary{% endblock %}" />
<meta name="twitter:title" content="{% block metaTwitterTitle %}{{ getContent().name|raw_desc }}{% endblock %}" />
<meta name="twitter:description" content="{% block metaTwitterDescription %}{{ getContent().meta_description|raw_desc }}{% endblock %}" />
<meta name="twitter:image" content="{% block metaTwitterImage %}{% image 'images/circle_picture.png' %}{{ app.request.getSchemeAndHttpHost() ~ asset_url }}{% endimage %}{% endblock %}" />
<meta name="twitter:site" content="{% block metaTwitterSite %}@{{ getContent().pseudo|raw_desc }}{% endblock %}" />
<meta name="twitter:creator" content="{% block metaTwitterCreator %}@{{ getContent().pseudo|raw_desc }}{% endblock %}" />
