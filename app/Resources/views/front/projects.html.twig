{% extends 'base.html.twig' %}

{% block title %}Projets{% endblock %}

{% block content %}
    <div id="projects">
        <div class="wrapper">
            {% if is_granted('ROLE_SUPER_ADMIN') %}
                <h3 class="bold">Projets</h3>
                <ul>
                    {% for project in projects %}
                        <li>{{ project.title }}</li>
                    {% endfor %}
                </ul>
            {% else %}
                {% include 'partials/construction.html.twig' %}
            {% endif %}
        </div>
    </div>
{% endblock %}
